const mongoose = require('mongoose')

const AdminSchema = new mongoose.Schema({
    AdminID: {
        type: Number,
        require: true
    },
    UserID: {
        type: Number,
        require: true
    },
    Email: {
        type: String,
        require: true
    },
    isAdmin: {
        type: Boolean,
        require: true
    }
})

module.exports = mongoose.model("Admin", AdminSchema);
